[
  {
    "name":"wikipedia",
    "match":"^(.{2,3})\\.m\\.wikipedia\\.org$",
    "replace":"$1.wikipedia.org"
  },
  {
    "name":"weather-underground",
    "match":"^i\\.wund\\.com$",
    "replace":"wunderground.com"
  },
  {
    "name":"reddit",
    "match":"i\\.reddit\\.com",
    "replace":"reddit.com"
  },
  {
    "name":"mobile_prefix",
    "match":"mobile\\.(([a-zA-Z0-9\\-_]+\\.)+([a-zA-Z0-9\\-_]+))",
    "replace":"$1"
  },
  {
    "name":"m_prefix",
    "match":"m\\.(([a-zA-Z0-9\\-_]+\\.)+([a-zA-Z0-9\\-_]+))",
    "replace":"$1"
  }
]
